<?xml version="1.0"?>
<widget xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:gap="http://phonegap.com/ns/1.0" xmlns="http://www.w3.org/ns/widgets" id="nlt.test.AppConstructorDebug" version="0.0.1" android-versionCode="1" ios-CFBundleVersion="0.0.1">

  <name>AppConstructor (Debug View Tool)</name>
  <description>This is application for testing your application created on AppConstructor.</description>
  <author email="a.mazepa@newline.tech" href="http://appconstructor.newline.tech/">AppConstructor</author>
  <plugin name="cordova-plugin-inappbrowser" spec="1.5.0" source="npm" />
  <plugin name="cordova-plugin-file-transfer" spec="1.6.0" source="npm" />
  <plugin name="cordova-plugin-network-information" spec="1.3.0" source="npm" />
  <plugin name="cordova-plugin-whitelist" spec="1.3.2" source="npm" />
  <plugin name="cordova-plugin-x-toast" spec="2.5.2" source="npm" />
  <plugin name="cordova-plugin-document-viewer" spec="0.8.1" source="npm" />
  <plugin name="cordova-plugin-statusbar" spec="2.2.0" source="npm" />
  <plugin name="cordova-plugin-splashscreen" spec="4.0.2" source="npm" />
  <plugin name="phonegap-plugin-barcodescanner" spec="6.0.8" source="npm" />
  <!--<plugin name="com.lampa.startapp" spec="0.1.4" source="npm"/>-->
  

  <!--  Ios splash-->

  <!--  Ios splash-->
  <allow-navigation href="http://*/*" />
  <allow-navigation href="https://*youtube.com/*"/>
  <allow-intent href="*" />
  <access origin="*" />
  <feature name="http://api.phonegap.com/1.0/file">
    <param name="" value="" />
  </feature>
  <feature name="http://api.phonegap.com/1.0/network">
    <param name="" value="" />
  </feature>
  <feature name="File">
    <param name="android-package" value="org.apache.cordova.file.FileUtils" />
  </feature>
  <feature name="Toast">
    <param name="android-package" value="nl.xservices.plugins.Toast" />
  </feature>
  <feature name="Toast">
    <param name="ios-package" value="Toast" />
  </feature>
  <feature name="Toast">
    <param name="wp-package" value="Toast" />
  </feature>
  <feature name="FileTransfer">
    <param name="android-package" value="org.apache.cordova.filetransfer.FileTransfer" />
  </feature>
  <preference name="show-splash-screen-spinner" value="false" />
  <!-- ios: if set to false, the spinner won't appear on the splash screen during app loading -->
  <preference name="auto-hide-splash-screen"    value="true" />
  <preference name="SplashScreen" value="screen"/>
  <preference name="SplashScreenDelay" value="3000"/>
  <preference name="orientation" value="default" />
  <preference name="phonegap-version" value="cli-7.0.1" />

<!--<splash src="splash.png" />-->
<platform name="ios" />
<platform name="android">
  <!--Icons-->
  <icon src="icons/icon.png" gap:role="default" />
  <!--Android-->
  <icon src="icons/android/ldpi.png" width="36" height="36"  qualifier="ldpi"  />
  <icon src="icons/android/mdpi.png" width="48" height="48"  qualifier="mdpi"  />
  <icon src="icons/android/hdpi.png" width="72" height="72"  qualifier="hdpi"  />
  <icon src="icons/android/xhdpi.png" width="96" height="96"  qualifier="xhdpi"  />

  <!--  Android splash -->
  <splash src="screen/android/screen-hdpi-landscape.png"  density="land-hdpi"/>
  <splash src="screen/android/screen-ldpi-landscape.png"  density="land-ldpi"/>
  <splash src="screen/android/screen-mdpi-landscape.png"  density="land-mdpi"/>
  <splash src="screen/android/screen-xhdpi-landscape.png"  density="land-xhdpi"/>
  <splash src="screen/android/screen-hdpi-portrait.png"  density="port-hdpi"/>
  <splash src="screen/android/screen-ldpi-portrait.png"  density="port-ldpi"/>
  <splash src="screen/android/screen-mdpi-portrait.png"  density="port-mdpi"/>
  <splash src="screen/android/screen-xhdpi-portrait.png"  density="port-xhdpi"/>
  <!--  Android splash -->
</platform>

  
