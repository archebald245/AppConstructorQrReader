<?xml version="1.0"?>
<widget xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:gap="http://phonegap.com/ns/1.0" xmlns="http://www.w3.org/ns/widgets" id="net.newlinetechnologies.appconstructor">
  <name>AppConstructor Debug</name>
  <description>This is application for testing your application created on AppConstructor.</description>
  <plugin name="com.fantasyskiracer.inappbrowser" spec="1.0.4" source="pgb" />
  <plugin name="org.apache.cordova.file-transfer" spec="0.5.0" source="pgb" />
  <plugin name="org.apache.cordova.network-information" spec="0.2.7" source="pgb" />
  <plugin name="cordova-plugin-whitelist" spec="" source="npm" />
  <plugin name="cordova-plugin-x-toast" spec="" source="npm" />
  <plugin name="org.apache.cordova.statusbar" spec="0.1.4" source="pgb" />
  <gap:plugin name="com.phonegap.plugins.barcodescanner" />


  <allow-navigation href="http://*/*" />
  <allow-intent href="*" />
  <access origin="*" />
  <feature name="http://api.phonegap.com/1.0/file">
    <param name="" value="" />
  </feature>
  <feature name="http://api.phonegap.com/1.0/network">
    <param name="" value="" />
  </feature>
  <feature name="File">
    <param name="android-package" value="org.apache.cordova.file.FileUtils" />
  </feature>
  <feature name="Toast">
    <param name="android-package" value="nl.xservices.plugins.Toast" />
  </feature>
  <feature name="Toast">
    <param name="ios-package" value="Toast" />
  </feature>
  <feature name="Toast">
    <param name="wp-package" value="Toast" />
  </feature>
  <feature name="FileTransfer">
    <param name="android-package" value="org.apache.cordova.filetransfer.FileTransfer" />
  </feature>
  <gap:platform name="android" />
<gap:platform name="ios" />
  <preference name="orientation" value="default" />
  <preference name="phonegap-version" value="cli-5.4.1" />
  <!--Icons-->
    <!--Android-->
  <icon src="icons/android/ldpi.png" gap:density="ldpi" gap:platform="android" />
  <icon src="icons/android/mdpi.png" gap:density="mdpi" gap:platform="android" />
  <icon src="icons/android/hdpi.png" gap:density="hdpi" gap:platform="android" />
  <icon src="icons/android/xhdpi.png" gap:density="xhdpi" gap:platform="android" />
    <!--Ios-->
  <icon src="icons/ios/App-29.png" width="29" height="29" gap:platform="ios" />
  <icon src="icons/ios/App-40.png" width="40" height="40" gap:platform="ios" />
  <icon src="icons/ios/App-57.png" width="57" height="57" gap:platform="ios" />
  <icon src="icons/ios/App-58.png" width="58" height="58" gap:platform="ios" />
  <icon src="icons/ios/App-72.png" width="72" height="72" gap:platform="ios"  />
  <icon src="icons/ios/App-90.png" width="90" height="90" gap:platform="ios"  />
  <icon src="icons/ios/App-100.png" width="100" height="100" gap:platform="ios"  />
  <icon src="icons/ios/App-114.png" width="114" height="114" gap:platform="ios" />
  <icon src="icons/ios/App-120.png" width="120" height="120" gap:platform="ios" />
  <icon src="icons/ios/App-144.png" width="144" height="144" gap:platform="ios" />
  <icon src="icons/ios/App-152.png" width="152" height="152" gap:platform="ios" />
  <icon src="icons/ios/App-180.png" width="180" height="180" gap:platform="ios" />
  <!--Icons-->
</widget>
