<?xml version="1.0"?>
<widget xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:gap="http://phonegap.com/ns/1.0" xmlns="http://www.w3.org/ns/widgets" id="nlt.test.AppConstructorDebug" version="0.0.1" android-versionCode="0.0.1" ios-CFBundleVersion="0.0.1">

  <name>AppConstructor(Debug View Tool)</name>
  <description>This is application for testing your application created on AppConstructor.</description>
  <author email="a.mazepa@newline.tech" href="http://appconstructor.newline.tech/">AppConstructor</author>
  <plugin name="cordova-plugin-inappbrowser" spec="1.5.0" source="npm" />
  <plugin name="cordova-plugin-file-transfer" spec="1.6.0" source="npm" />
  <plugin name="cordova-plugin-network-information" spec="1.3.0" source="npm" />
  <plugin name="cordova-plugin-whitelist" spec="1.3.0" source="npm" />
  <plugin name="cordova-plugin-x-toast" spec="2.5.2" source="npm" />
  <plugin name="cordova-plugin-statusbar" spec="2.2.0" source="npm" />
  <plugin name="cordova-plugin-splashscreen" spec="4.0.2" source="npm" />
  <plugin name="phonegap-plugin-barcodescanner" spec="6.0.6" source="npm" />
  <!--<plugin name="com.lampa.startapp" spec="0.1.4" source="npm"/>-->
  <plugin name="cordova-plugin-document-viewer" spec="0.8.1" source="npm" />


  <allow-navigation href="http://*/*" />
  <allow-intent href="*" />
  <access origin="*" />
  <feature name="http://api.phonegap.com/1.0/file">
    <param name="" value="" />
  </feature>
  <feature name="http://api.phonegap.com/1.0/network">
    <param name="" value="" />
  </feature>
  <feature name="File">
    <param name="android-package" value="org.apache.cordova.file.FileUtils" />
  </feature>
  <feature name="Toast">
    <param name="android-package" value="nl.xservices.plugins.Toast" />
  </feature>
  <feature name="Toast">
    <param name="ios-package" value="Toast" />
  </feature>
  <feature name="Toast">
    <param name="wp-package" value="Toast" />
  </feature>
  <feature name="FileTransfer">
    <param name="android-package" value="org.apache.cordova.filetransfer.FileTransfer" />
  </feature>
  <preference name="show-splash-screen-spinner" value="false" />           <!-- ios: if set to false, the spinner won't appear on the splash screen during app loading -->
   <preference name="auto-hide-splash-screen"    value="true" />
  <preference name="SplashScreen" value="screen"/>
<preference name="SplashScreenDelay" value="1000"/>
  <gap:platform name="android" />
<!--<gap:platform name="ios" />-->
  <preference name="orientation" value="default" />
  <preference name="phonegap-version" value="cli-6.5.0" />
  <!--Icons-->
  <icon src="icons/icon.png" gap:role="default" />
    <!--Android-->
  <icon src="icons/android/ldpi.png" width="36" height="36"  qualifier="ldpi" platform="android" />
  <icon src="icons/android/mdpi.png" width="48" height="48"  qualifier="mdpi" platform="android" />
  <icon src="icons/android/hdpi.png" width="72" height="72"  qualifier="hdpi" platform="android" />
  <icon src="icons/android/xhdpi.png" width="96" height="96"  qualifier="xhdpi" platform="android" />
    <!--Ios-->
  <icon src="icons/ios/App-29.png" width="29" height="29" qualifier="" platform="ios" />
  <icon src="icons/ios/App-40.png" width="40" height="40" qualifier="" platform="ios" />
  <icon src="icons/ios/App-57.png" width="57" height="57" qualifier="" platform="ios" />
  <icon src="icons/ios/App-58.png" width="58" height="58" qualifier="" platform="ios" />
  <icon src="icons/ios/App-72.png" width="72" height="72" qualifier="" platform="ios"  />
  <icon src="icons/ios/App-90.png" width="90" height="90" qualifier="" platform="ios"  />
  <icon src="icons/ios/App-100.png" width="100" height="100" qualifier="" platform="ios"  />
  <icon src="icons/ios/App-114.png" width="114" height="114" qualifier="" platform="ios" />
  <icon src="icons/ios/App-120.png" width="120" height="120" qualifier="" platform="ios" />
  <icon src="icons/ios/App-144.png" width="144" height="144" qualifier="" platform="ios" />
  <icon src="icons/ios/App-152.png" width="152" height="152" qualifier="" platform="ios" />
  <icon src="icons/ios/App-180.png" width="180" height="180" qualifier="" platform="ios" />
  <!--Icons-->

  <!--  Android splash -->
  <splash src="screen/android/screen-hdpi-landscape.png" platform="android" density="land-hdpi"/>
  <splash src="screen/android/screen-ldpi-landscape.png" platform="android" density="land-ldpi"/>
  <splash src="screen/android/screen-mdpi-landscape.png" platform="android" density="land-mdpi"/>
  <splash src="screen/android/screen-xhdpi-landscape.png" platform="android" density="land-xhdpi"/>
  <splash src="screen/android/screen-hdpi-portrait.png.png" platform="android" density="port-hdpi"/>
  <splash src="screen/android/screen-ldpi-portrait.png" platform="android" density="port-ldpi"/>
  <splash src="screen/android/screen-mdpi-portrait.png" platform="android" density="port-mdpi"/>
  <splash src="screen/android/screen-xhdpi-portrait" platform="android" density="port-xhdpi"/>
  <!--  Android splash -->

  <!--  Ios splash-->
  <platform name="ios">
  <splash src="screen/ios/screen-iphone-portrait.png"         platform="ios" width="320" height="480" />
  <splash src="screen/ios/screen-iphone-portrait-2x.png"      platform="ios" width="640" height="960" />
  <splash src="screen/ios/screen-iphone-portrait-568h-2x.png" platform="ios" width="640" height="1136" />
  <splash src="screen/ios/screen-ipad-portrait.png"           platform="ios" width="768" height="1024" />
  <splash src="screen/ios/screen-ipad-landscape.png"          platform="ios" width="1024" height="768" />
  <splash src="screen/ios/w750xh1334.png"          platform="ios" width="750" height="1334" />
  <splash src="screen/ios/w1242xh2208.png"          platform="ios" width="1242" height="2208" />
  <splash src="screen/ios/w1536xh2048.png"          platform="ios" width="1536" height="2048" />
  <splash src="screen/ios/w2048xh1536.png"          platform="ios" width="2048" height="1536" />
  <splash src="screen/ios/w2208xh1242.png"          platform="ios" width="2208" height="1242" />
  </platform>
  <!--  Ios splash-->
</widget>
